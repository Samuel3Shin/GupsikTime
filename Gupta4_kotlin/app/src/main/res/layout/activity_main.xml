                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/breakfast" />

            <TextView
                android:id="@+id/breakfastTextView_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="현미밥" android:layout_marginTop="8dp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@+id/down_breakfast_line"
                app:layout_constraintBottom_toTopOf="@+id/breakfastTextView_3"
                app:layout_constraintStart_toStartOf="@id/meal_info_guideline1" />


            <TextView
                android:id="@+id/breakfastTextView_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="핫도그"
                android:textColor="@color/black"
                app:layout_constraintTop_toTopOf="@+id/breakfastTextView_1"
                app:layout_constraintStart_toStartOf="@id/meal_info_guideline2"
                app:layout_constraintBottom_toBottomOf="@+id/breakfastTextView_1" />


            <TextView
                android:id="@+id/breakfastTextView_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="현미밥" android:layout_marginTop="8dp"
                android:textColor="@color/black"
                app:layout_constraintBottom_toTopOf="@+id/breakfastTextView_5"
                app:layout_constraintStart_toStartOf="@id/meal_info_guideline1"
                app:layout_constraintTop_toBottomOf="@+id/breakfastTextView_1" />


            <TextView
                android:id="@+id/breakfastTextView_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="핫도그"
                android:textColor="@color/black"
