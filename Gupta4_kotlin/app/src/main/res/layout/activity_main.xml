                app:layout_constraintBottom_toBottomOf="@+id/breakfastTextView_11" />

            <ImageView
                android:id="@+id/upper_lunch_line"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="2dp"
                android:layerType="software"
                android:src="@drawable/dotted_line_horizontal_wider"
                app:layout_constraintTop_toBottomOf="@+id/breakfastTextView_11"
                app:layout_constraintBottom_toTopOf="@+id/lunch"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/lunch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="점심식사"
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/upper_lunch_line"
                app:layout_constraintBottom_toTopOf="@+id/down_lunch_line" />

            <ImageView
                android:id="@+id/down_lunch_line"
                android:layout_width="match_parent"
                android:layout_height="2dp"
